// Breakpoints
screen-sm = 576px
screen-md = 768px
screen-lg = 992px
screen-xl = 1200px

// Opacity
high-emphasis = 0.87
medium-emphasis = 0.6
low-emphasis = 0.38

// Colors
primary-color = #616161
primary-dark-color = #373737
